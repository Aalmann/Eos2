  </en>
  </template> 
  <template de="Umkehrsinus" en="inverse sinus">
  <de>
    arcsin(Wert)
  </de>
  <en>
    arcsin(value)
  </en>
  </template> 
  <template de="Umkehrkosinus" en="inverse cosinus">
  <de>
    arccos(Wert)
  </de>
  <en>
    arccos(value)
  </en>
  </template>  
  <template de="Polarkoordinate Winkel" en="polar coordinate angle">
  <de>
    phi(x,y)
  </de>
  <en>
    phi(x,y)
  </en>
  </template> 
  <template de="Polarkoordinate Abstand" en="polar coordinate distance">
