<templates>
  <template de="Endlosschleife" en="infinite loop">
  <de>
    wiederhole immer
    
    *wiederhole
  </de>
  <en>
    repeat forever
    
    *repeat  
  </en>
  </template>
  <template de="Zählschleife" en="counting loop">
  <de>
    wiederhole Anzahl mal
    
    *wiederhole  
  </de>
  <en>
    repeat count times
    
    *repeat  
  </en>
  </template> 
  <template de="Bedingte Schleife" en="conditional loop">
  <de>
    wiederhole solange Bedingung
    
    *wiederhole
  </de>
  <en>
    repeat while condition
    
    *repeat
  </en>
  </template> 
  <template de="Bedingte Anweisung" en="conditional statement">
  <de>
    wenn Bedingung dann
    
    *wenn
  </de>
  <en>
    if condition then
    
    *if
  </en>
  </template>  
  <template de="Alternative Anweisungen" en="alternative statement">
  <de>
    wenn Bedingung dann
    
    sonst
    
    *wenn
  </de>
  <en>
    if condition then
    
    else
    
    *if
  </en>
  </template> 
  <template de="Schleife mit Endbedingung" en="loop with condition at its end">
  <de>
    wiederhole
    
    *wiederhole solange Bedingung
  </de>
  <en>
    repeat
    
    *repeat while condition
  </en>
  </template> 
  <template de="Runden" en="round">
  <de>
    round(Zahl)
  </de>
  <en>
    round(number)
  </en>
  </template> 
  <template de="Abrunden" en="floor">
